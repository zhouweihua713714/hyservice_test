{
  "name": "hy-server",
  "version": "1.0.0",
  "description": "hy services built with nest architecture",
  "author": "hana",
  "private": true,
  "license": "UNLICENSED",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test  --passWithNoTests --forceExit"
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:test": "cross-env NODE_ENV=test node dist/main",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" ",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "tsorm": "yarn ts-node node_modules/typeorm/cli.js",
    "m:gen": "yarn tsorm migration:generate -d src/dataSource.migration.ts",
    "m:check": "yarn tsorm migration:generate -d src/dataSource.migration.ts new --ch",
    "m:create": "yarn tsorm migration:create",
    "m:revert": "yarn tsorm migration:revert -d src/dataSource.migration.ts",
    "m:run": "yarn tsorm migration:run -d src/dataSource.migration.ts",
    "m:gen-diff": "yarn tsorm m:run | yarn tsorm m:gen diff"
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@liaoliaots/nestjs-redis": "^5.2.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "ali-oss": "^6.17.1",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-fetch": "^3.1.5",
    "csurf": "^1.11.0",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.2",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.5",
    "log4js": "^6.3.0",
    "mime-types": "^2.1.34",
    "nestjs": "^0.0.1",
    "nestjs-redis": "^1.3.3",
    "node-xlsx": "^0.21.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path-to-regexp": "^6.2.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.1",
    "shortid": "^2.2.16",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.3.0",
    "ts-enum-util": "^4.0.2",
    "typeorm": "^0.3.6",
    "typeorm-extension": "^2.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/ali-oss": "^6.16.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.179",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.19",
    "@types/node-xlsx": "^0.15.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "4.3.8",
    "jest": "^27.4.7",
    "npx": "^10.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^4.5.4"
  },
  "jest": {
    "maxWorkers": 2,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pre-commit": [
    "m:check"
  ]
}
